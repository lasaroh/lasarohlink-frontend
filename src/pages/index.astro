---
import Layout from "../layouts/Layout.astro";
import FormUrlShortener from "../components/FormUrlShortener.astro";
---

<Layout title="LasarohLink">
  <div>
    <div class="flex mt-24 mb-5 justify-center items-center">
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="24"
        height="24"
        viewBox="0 0 24 24"
        fill="none"
        stroke="currentColor"
        stroke-width="2"
        stroke-linecap="round"
        stroke-linejoin="round"
        class="icon icon-tabler icons-tabler-outline icon-tabler-link w-12 h-12 text-blue-500"
      >
        <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
        <path d="M9 15l6 -6"></path>
        <path d="M11 6l.463 -.536a5 5 0 0 1 7.071 7.072l-.534 .464"></path>
        <path
          d="M13 18l-.397 .534a5.068 5.068 0 0 1 -7.127 0a4.972 4.972 0 0 1 0 -7.071l.524 -.463"
        ></path>
      </svg>
      <h1 class="text-5xl text-blue-500 font-thin ml-4">LasarohLink</h1>
      <h2 class="mt-7 text-sm text-blue-500">alpha</h2>
    </div>
    <FormUrlShortener />
  </div>
</Layout>
